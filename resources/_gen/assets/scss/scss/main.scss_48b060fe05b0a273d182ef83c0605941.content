:root{font:20px/1.2 roboto,sans-serif}html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{margin:unset;padding:unset;border:unset;font-size:unset;vertical-align:baseline;box-sizing:unset;color:unset;text-decoration:unset}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none}table{border-collapse:collapse;border-spacing:0}h2,p{margin-bottom:2.4rem}h1{font:700 2rem/1.2 roboto;color:#291e2f}h2{font:1.5rem/1.2 roboto;color:#291e2f}time{font:700 1rem/1.2 roboto;color:#574d5b}h3{font:700 1rem/1.2 roboto;color:darkgrey}p{font:.95rem/1.5 roboto;color:#1e191f}summary{font:.95rem/1.5 roboto;color:#1e191f}span{font:500 .9rem/1.5 Inconsolata;color:#574d5b}figcaption p{font:1rem/1.5 roboto;color:#574d5b}pre *{font:.8rem/1.5 Inconsolata}a{color:#7d02a7;box-shadow:inset 0 -.1rem #7d02a7,0 .1rem #7d02a7;transition:box-shadow .2s}a:hover{color:#9002c0;box-shadow:inset 0 -.15rem #9002c0,0 .15rem #9002c0}@media(max-width:8.5in){:root{font-size:18px}}@media(max-width:5in){:root{font-size:16px}}body{min-height:100vh;display:grid;grid-template-columns:1fr 0.5in [start] 1.25in 5in 1.25in [end] 0.5in 1fr;grid-template-rows:auto auto 1fr auto}header{grid-row:1;grid-column:start/span 2}nav{grid-row:1;grid-column:4/end;justify-self:right}.content{grid-row:3;grid-column:1/-1;margin-top:3.5rem}footer{grid-row:-1;grid-column:start/end}.header-background{grid-row:1;grid-column:1/-1;z-index:-1}@media(max-width:8.5in){body{grid-template-columns:1fr 5% [start] 15% 60% 15% [end] 5% 1fr}header{grid-row:1;grid-column:start/end;justify-self:center}nav{grid-row:2;grid-column:start/end;justify-self:center}.header-background{grid-row:1/span 2}}header .logo{font:700 2rem/1.2 roboto;color:#291e2f}header{padding:1.5rem 0}header a{box-shadow:none}.header-background{background-color:#eee;height:100%}@media(max-width:8.5in){.header-background{padding-bottom:2rem}}nav{font-size:1.3rem;align-self:center}article .size-1{grid-column:4/5}article .size-2{grid-column:3/6}article .size-3{grid-column:2/7}article .size-4{grid-column:1/8}article .size-1 img,article .size-2 img,article .size-3 img,article .size-4 img{width:100%}article figcaption{color:#574d5b;margin-top:.1rem;margin-bottom:1.5rem;display:flex;justify-content:center}article{display:grid;grid-template-columns:1fr 0.5in [start] 1.25in 5in 1.25in [end] 0.5in 1fr}article *{grid-column:start/end}@media(max-width:8.5in){article{grid-template-columns:1fr 5% [start] 15% 60% 15% [end] 5% 1fr}}.overview-container{display:grid;grid-template-columns:1fr 0.5in [start] 1.25in 5in 1.25in [end] 0.5in 1fr}.overview-container *{grid-column:start/end}@media(max-width:8.5in){.overview-container{grid-template-columns:1fr 5% [start] 15% 60% 15% [end] 5% 1fr}}.overview-items .overview-item{margin-top:0;margin-bottom:1rem;padding:1.5rem 1rem 2.5rem;border-bottom:2px solid #d1c9d4}.overview-items .overview-item h1{margin-bottom:.3rem}.overview-items .overview-item summary{margin-top:.5rem}.overview-items .overview-item span{display:block;margin-top:.5rem}.overview-items .overview-item span a{color:#574d5b;box-shadow:inset 0 -.05rem #574d5b,0 .05rem #574d5b;transition:box-shadow .2s}.overview-items .overview-item span a:hover{color:#9002c0;box-shadow:inset 0 -.1rem #9002c0,0 .1rem #9002c0}.category-container{display:flex;justify-content:center}footer{font-weight:400;font-size:.9rem;color:grey;justify-self:center}